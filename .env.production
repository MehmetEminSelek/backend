# Database URL'i Hostinger'daki PostgreSQL veritabanı bilgileriyle güncellenecek
DATABASE_URL="postgresql://username:password@hostname:5432/database_name?schema=public"

# Production ortamı
NODE_ENV=production

# API URL'leri
NEXT_PUBLIC_API_URL=https://your-domain.com/api

# CORS ayarları
CORS_ORIGIN=https://your-frontend-domain.com

# Port ayarı (Hostinger'da genellikle otomatik atanır)
PORT=8080 